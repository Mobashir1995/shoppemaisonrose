/* Styles for a vertical volume scrollbar */
.plyr--full-ui .plyr__volume {
  background-color: {{ product_media_bg_color | color_modify: 'alpha', 0.25 }};
  border-radius: 5px;
  bottom: 105px;
  height: 40px;
