/* Flexbox */
.is-flex {
  display: flex;
}

.is-inline-flex {
  display: inline-flex;
}

.is-align-start,
.is-align-left {
  align-items: flex-start;
}

.is-align-end,
.is-align-right {
  align-items: flex-end;
}

.is-align-self-end,
.is-align-self-right {
  align-self: flex-end;
}

.is-align-baseline {
  align-items: baseline;
}

.is-align-center {
