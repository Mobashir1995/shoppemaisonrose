.product-category-card {
  margin: 10px;
  overflow: hidden;
  border: 1px solid #e9e9eb;
  border-radius: 5px;
}
.product-category-card figcaption {
  margin-top: -11px;
}
.product-category-card h4 {
  line-height: 24px;
  text-transform: inherit;
}
.product-category-card p {
  line-height: 20px;
  font-size: 14px;
}
.product-category-card .description-box {
  visibility: hidden;
  transition: all .45s ease-in-out;
  position: absolute;
  z-index: 2;
  top: 50%;
  opacity: 0; 
  left: 0;
  right: 0;   
}
.product-category-card:after {
  content: '';
  transition: all .45s ease-in-out;
  position: absolute;
  z-index: 1;
  visibility: hidden;
  background: #e9e9eb;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.product-category-card:hover .description-box{
  visibility: visible;
  top: 50%;
  transform: translateY(-50%);
  opacity: 1;
}
.product-category-card:hover:after {
  visibility: visible;
  opacity: 1;
}
.product-category-card .action_button {
  max-width: 155px;
  letter-spacing: 1px;
  text-transform: inherit;
}
.product-listing-container .product-category-card:after{
