    display: block;
    margin-top: 20px;
  }
  .collapsible-table tfoot tr {
    margin-bottom: 0;
    border-bottom: none;
  }
  .collapsible-table tfoot tr:last-child {
    border-bottom: 1px solid #e0e0e0;
  }
}
