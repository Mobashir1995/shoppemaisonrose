<!-- End Google Tag Manager -->
<meta name="google-site-verification" content="RfCwtPuFmHZo5qEXNarfmfb5i9uD__AgFc5wHeKcD3M" />
  
  </head>

  <body class="{{ template | replace: '.', ' ' | handle }}"
        data-money-format="
            {%- if settings.currency_format == 'money_with_currency_format' -%}
              {{ shop.money_with_currency_format | strip_html }}
            {%- else -%}
              {{ shop.money_format | strip_html }}
            {%- endif -%}"
        data-active-currency="{{ cart.currency.iso_code }}"
        data-shop-url="{{ routes.root_url }}">
   {% render 'elevar-body-end' %}
   <!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PHC3GJQ"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->
    <!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-WRCMBN5"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->

    {% section 'header' %}

    {{ content_for_layout }}

    {% render 'cart-shipping-calculator' %}

    {% section 'footer' %}

    {% if settings.newsletter_popup %}
      {% render 'newsletter-popup' %}
    {% endif %}

    {% if settings.size_chart != blank and template contains 'product' %}
      {% render 'size-chart-popup' %}
    {% endif %}

    {% render 'mobile-search' %}

    {% if settings.disqus_enabled %}
      {% if template contains "blog" or template contains "article" or template contains "index" %}
        <script id="dsq-count-scr" src="//{{ settings.disqus_shortname }}.disqus.com/count.js" async></script>
      {% endif %}
    {% endif %}
     {{'body-content.js' | asset_url | script_tag }}
  </body>
</html>
